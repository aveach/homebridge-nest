{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "array-callback-return": 0,
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": [
          "transformData"
        ]
      }
    ],
    "comma-dangle": 0,
    "func-names": 0,
    "max-len": [
      "error", {
        "code": 150
      }
    ],
    "new-cap": 0,
    "no-bitwise": 0,
    "no-continue": 0,
    "no-multi-assign": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-destructuring": [
      "error", {
        "array": true,
        "object": false
      }
    ],
    "prefer-arrow-callback": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "strict": 0,
    "vars-on-top": 0
  }
}
